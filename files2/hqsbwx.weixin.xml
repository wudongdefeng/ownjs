;&lt;/section&gt;&lt;section style=&quot;display: inline-block; vertical-align: middle; width: 33.33%;&quot;&gt;&lt;section style=&quot;margin: 0px 0%; line-height: 0;&quot;&gt;&lt;section style=&quot;vertical-align: middle; display: inline-block; line-height: 0; width: 100%;&quot;&gt;&lt;img class=&quot;rich_pages wxw-img&quot; src=&quot;http://img2.jintiankansha.me/get?src=http://mmbiz.qpic.cn/mmbiz_jpg/wlCrBZoK8HHUHIaaY5iahBk7BsQGB7ZzIdQjSiaqamibq5O8HibQvRKEWuBP7wB5obIeIzjgHOZJKmux3IM4kYc2XA/640?wx_fmt=jpeg&amp;amp;from=appmsg&quot; style=&quot;vertical-align: middle; width: 100%;&quot; /&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;section style=&quot;display: inline-block; vertical-align: middle; width: 33.33%;&quot;&gt;&lt;section style=&quot;margin: 0.5em 0px;&quot;&gt;&lt;section style=&quot;background-color: rgb(160, 160, 160); height: 1px;&quot;&gt;&lt;svg viewBox=&quot;0 0 1 1&quot; xmlns=&quot;http://www.w3.org/2000/svg&quot;&gt;&lt;/svg&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;/section&gt;&lt;section&gt;&lt;span&gt;&lt;br /&gt;&lt;/span&gt;&lt;/section&gt;&lt;p style=&quot;display: none;&quot;&gt;&lt;/p&gt;&lt;/div&gt;
    

    
